pipeline {
  agent any
  stages 
  {
    stage('Remove old deployment') 
    {
      steps {
        bat 'docker-compose down '
        }
    }
    stage('Remove old image build ') 
    {
         steps {
            bat 'docker image rm ayushishu_portfolio_website_wed'
             }
    }         
    stage('Build and deploy update image')
    {
          steps {
            bat 'docker-compose up -d'
          }
    }      

    
  
  } 
}

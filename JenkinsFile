job('who-is-ayush-wedsite-dev-branch') {
  description("dev-branch-CI-CD pipe-line for Ayushishu portfolio website")

  scm {
    git branch: 'dev', url: 'https://github.com/ayushishu/Ayushishu_Portfolio_website/'
    }
  
pipeline {
  agent any
  stages 
  {
    stage('Remove old deployment') 
    {
      steps {
        bat 'docker-compose down '
        }
    }       
    stage('Build and deploy update image')
    {
          steps {
            bat 'docker-compose up -d'
          }
    }      

    
  
  } 
}




}